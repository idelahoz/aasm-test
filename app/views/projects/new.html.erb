<h1>Nuevo Proyecto</h1>

<%= form_with model: @project, local: true do |form| %>
  <div>
    <%= form.label :name, "Nombre del proyecto" %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.submit "Crear Proyecto" %>
  </div>
<% end %>

<%= link_to 'Volver a la lista', projects_path %>
